<header>
  <div class="container">
    <ul>
      <li class="link"><%= link_to "Home", root_path %></li>
      <li class="link"><%= link_to "Users", users_path  %></li>
      <% if logged_in? %>
        <li class="link"><%= link_to "Log out",  logout_path, method: :post %></li>
        <li class="link"><%= link_to "map", new_review_path %></li>
        <li class="link"><%= link_to "reviews", reviews_path %></li>
      <% else %>
        <li class="link reactheader" >
          <%= react_component("Signup", {
            signup: users_path,
            login: login_path,
            authenticityToken: form_authenticity_token } )
          %>
        </li>
      <% end %>
    </ul>
  </div>
</header>
