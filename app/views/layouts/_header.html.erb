<header>
  <% if logged_in? %>
    <%= react_component("Loginheader", {
      authenticityToken: form_authenticity_token,
      logout: logout_path,
      Yourpage: user_path(@current_user),
    } )%>
  <% else %>
    <%= react_component("Header", {
      authenticityToken: form_authenticity_token,
      signup: users_path,
      login: login_path,
    } )%>
  <% end %>
</header>
