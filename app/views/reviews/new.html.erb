<div class="title">
  <h1>あなたの海外経験をシェアをしよう！</h1>
</div>
<%= form_with( url: check_place_path, remote: true) do |f| %>
  <div class="field" style="display:inline-block;" >
    <%= f.text_field :keyword, :placeholder => "場所を入力してください" , class:"window" %>
  </div>
   <div class="actions" style="display:inline-block;">
    <%= f.submit value="検索" %>
  </div>
<% end %>
<%= render partial: "googlemap" , locals: { lat: session[:lat], lng: session[:lng] } %>
