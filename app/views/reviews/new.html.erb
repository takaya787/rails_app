  <div class="title">
    <h1>評価を投稿してみよう！</h1>
  </div>

  <div id="flash_messages">
  </div>

  <% if @review %>
    <%= render 'errors',  review: @review %>
  <% end %>

  </div>
  <div id="google-map">
    <%= react_component("Map", {
      authenticityToken: form_authenticity_token,
      currentUser: @current_user.id,
    })  %>
  </div>
</div>
