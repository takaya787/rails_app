<h1>Log in</h1>
<%= form_with( url: login_path, scope: :session, local: true) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>

  <div class="field">
    <%= form.label :password_confirmation %>
    <%= form.text_field :password_confirmation %>
  </div>

  <div class="actions">
    <%= form.submit value="Log in" %>
  </div>
<% end %>
<%= link_to "GoogleでLog in","/auth/google_oauth2" %>
<%= link_to "Back", root_url %>
